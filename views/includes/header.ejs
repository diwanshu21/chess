<style>
  @import url('https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600;700;800;900&display=swap');
  * {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  letter-spacing: 1px;
  font-family: 'Poppins', sans-serif;
}

body {
  width: 100%;
  margin: auto;
  background: #fafafa;
}

.container {
  max-width: 1100px;
  margin: auto;
}

ul {
  list-style-type: none;
}

h1{
    font-size: 30px;
}

a {
  text-decoration: none;
}

header {
  width: 100%;
  padding: 10px 20px;
  display: flex;
  justify-content: space-between;
  border: 2px solid black;

}

header .logo {
  width: 50px;
}

header .logo img {
  width: 100%;
}

header nav ul {
  display: flex;
}

header nav li {
  margin: 10px;
  padding: 10px;
  font-size: 20px;
  border: 2px solid black;
  background-color: rgb(21, 0, 255);
}

header nav a {
  color: white;
}
</style>
<header>
    <div class="logo">
      <a href="/"
        ><img
          src="https://cdn4.vectorstock.com/i/1000x1000/01/08/logic-chess-game-logo-simple-style-vector-22940108.jpg"
          alt="chess logo"
      /></a>
    </div>
    <nav>
      <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/login">Login</a></li>
        <li><a href="/register">Register</a></li>
        <li class="logout"><a>Logout</a></li>
      </ul>
    </nav>
  </header>
  <script>
let logout=document.querySelector('.logout');

logout.addEventListener('click',async(e)=>{
  e.preventDefault();

  fetch('/api/v1/logout').then(res=>res.json()).then(data=>{
    console.log(data)
    location.reload();
    location.href='/'
  }).catch(err=>{
    console.log(err);
  })

})
  </script>
